{
  "manifest_version": 3,
  "name": "Selection Extractor",
  "version": "1.0",
  "permissions": [
    "storage" 
  ],
  "content_scripts": [ 
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]      
    }
  ],
  "background": { 
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}